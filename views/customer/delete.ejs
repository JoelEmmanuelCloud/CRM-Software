
	<form method="post">
		<fieldset>
			<legend>Delete Customer</legend>
			<table>
				<tr>
					<td>Customer Name  :</td>
					<td><%=customerName%></td>
				</tr>
				<tr>
					<td>Contact Number :</td>
					<td><%=customerContactNumber%></td>
				</tr>
				<tr>
					<td>Address :</td>
					<td><%=customerAddress%></td>
				</tr>
				
			</table>
			<div>
				<h3>Are you sure?</h3>
				<input type="submit" name="submit" value="Confirm">
			</div>
		</fieldset>
	</form>
